	<animation>
      <type>material</type>
      <shininess>116</shininess>
      <specular>
        <red>1</red>
        <green>1</green>
        <blue>1</blue>
      </specular>
      <ambient>
        <red>1</red>
        <green>1</green>
        <blue>1</blue>
      </ambient>
      <diffuse>
        <red>1</red>
        <green>1</green>
        <blue>1</blue>
      </diffuse>
      <object-name>Fin</object-name>
      <object-name>Wings</object-name>
      <object-name>Aileron.R</object-name>
      <object-name>Aileron.L</object-name>
      <object-name>Fairing</object-name>
      <object-name>HTail</object-name>
      <object-name>Elevator</object-name>
      <object-name>Rudder</object-name>
      <object-name>Nose</object-name>
	</animation>

	<effect>
	  <inherits-from>Effects/lak16-reflect</inherits-from>
      <object-name>Fin</object-name>
      <object-name>Wings</object-name>
      <object-name>Aileron.R</object-name>
      <object-name>Aileron.L</object-name>
      <object-name>Fairing</object-name>
      <object-name>HTail</object-name>
      <object-name>Rudder</object-name>
      <object-name>Elevator</object-name>
      <object-name>Nose</object-name>
      <object-name>Frame</object-name>
	</effect>

    <!--effect>
      <inherits-from>Effects/bumpspec-htail</inherits-from>
      <object-name>Elevator</object-name>
    </effect-->


	<animation>
	  <type>rotate</type>
      <object-name>LG.Spring.L.1</object-name>
      <object-name>Axis.Spring.L.2</object-name>
      <object-name>LG.Spring.L.2</object-name>
	  <object-name>LG.Wheel.L</object-name>
      <object-name>Axis.Axle.L</object-name>
	  <property>/gear/gear[1]/compression-ft</property>
	  <interpolation>
		<entry>
		  <ind>0.0</ind>
		  <dep>0.0</dep>
		</entry>
		<entry>
		  <ind>0.164</ind>
		  <dep>5.52</dep>
		</entry>
		<entry>
		  <ind>0.328</ind>
		  <dep>11.13</dep>
		</entry>
		<entry>
		  <ind>0.492</ind>
		  <dep>15.0</dep>
		</entry>
	  </interpolation>
	  <axis>
		<object-name>Axis.Spring.L.1</object-name>
	  </axis>
	</animation>


	<animation>
	  <type>rotate</type>
      <object-name>LG.Spring.L.2</object-name>
	  <object-name>LG.Wheel.L</object-name>
      <object-name>Axis.Axle.L</object-name>
	  <property>/gear/gear[1]/compression-ft</property>
	  <interpolation>
		<entry>
		  <ind>0.0</ind>
		  <dep>0.0</dep>
		</entry>
		<entry>
		  <ind>0.164</ind>
		  <dep>4.0</dep>
		</entry>
		<entry>
		  <ind>0.328</ind>
		  <dep>10.0</dep>
		</entry>
		<entry>
		  <ind>0.492</ind>
		  <dep>12.0</dep>
		</entry>
	  </interpolation>
	  <axis>
		<object-name>Axis.Spring.L.2</object-name>
	  </axis>
	</animation>

	<animation>
	  <type>spin</type>
	  <object-name>LG.Wheel.L</object-name>
	  <property>/gear/gear[1]/rollspeed-ms</property>
	  <factor>-35</factor>
	  <axis>
		<object-name>Axis.Axle.L</object-name>
	  </axis>
	</animation>




	<animation>
	  <type>rotate</type>
      <object-name>LG.Spring.R.1</object-name>
      <object-name>Axis.Spring.R.2</object-name>
      <object-name>LG.Spring.R.2</object-name>
	  <object-name>LG.Wheel.R</object-name>
      <object-name>Axis.Axle.R</object-name>
	  <property>/gear/gear[2]/compression-ft</property>
	  <interpolation>
		<entry>
		  <ind>0.0</ind>
		  <dep>0.0</dep>
		</entry>
		<entry>
		  <ind>0.164</ind>
		  <dep>-5.52</dep>
		</entry>
		<entry>
		  <ind>0.328</ind>
		  <dep>-11.13</dep>
		</entry>
		<entry>
		  <ind>0.492</ind>
		  <dep>-15.0</dep>
		</entry>
	  </interpolation>
	  <axis>
		<object-name>Axis.Spring.R.1</object-name>
	  </axis>
	</animation>


	<animation>
	  <type>rotate</type>
      <object-name>LG.Spring.R.2</object-name>
	  <object-name>LG.Wheel.R</object-name>
      <object-name>Axis.Axle.R</object-name>
	  <property>/gear/gear[2]/compression-ft</property>
	  <interpolation>
		<entry>
		  <ind>0.0</ind>
		  <dep>0.0</dep>
		</entry>
		<entry>
		  <ind>0.164</ind>
		  <dep>-4.0</dep>
		</entry>
		<entry>
		  <ind>0.328</ind>
		  <dep>-10.0</dep>
		</entry>
		<entry>
		  <ind>0.492</ind>
		  <dep>-12.0</dep>
		</entry>
	  </interpolation>
	  <axis>
		<object-name>Axis.Spring.R.2</object-name>
	  </axis>
	</animation>

	<animation>
	  <type>spin</type>
	  <object-name>LG.Wheel.R</object-name>
	  <property>/gear/gear[2]/rollspeed-ms</property>
	  <factor>35</factor>
	  <axis>
		<object-name>Axis.Axle.R</object-name>
	  </axis>
	</animation>


	<animation>
		<type>rotate</type>
		<object-name>Aileron.L</object-name>
		<property>/controls/flight/aileron</property>
		<interpolation>
			<entry>
				<ind>-1.0</ind>
				<dep>20.0</dep>
			</entry>
			<entry>
				<ind>0.0</ind>
				<dep>0.0</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>-10.0</dep>
			</entry>
		</interpolation>
		<axis>
			<object-name>Axis.Aileron.L</object-name>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>Aileron.R</object-name>
		<property>/controls/flight/aileron</property>
		<interpolation>
			<entry>
				<ind>-1.0</ind>
				<dep>10.0</dep>
			</entry>
			<entry>
				<ind>0.0</ind>
				<dep>0.0</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>-20.0</dep>
			</entry>
		</interpolation>
		<axis>
			<object-name>Axis.Aileron.R</object-name>
		</axis>
	</animation>



 </PropertyList>
